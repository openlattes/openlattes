{
  "name": "openlattes",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "test": "jest --watch",
    "lint": "eslint --ext .js,.jsx client/ server/",
    "buildserver": "babel server -d dist --env-name server",
    "buildclient": "parcel build client/index.html -d dist/public",
    "buildadmin": "yarn run buildserver && parcel build admin/index.html -d dist/public",
    "build": "yarn run buildserver && yarn run buildclient",
    "deploy": "now --public && now alias && now rm openlattes --safe --yes",
    "now-start": "NODE_ENV=production node dist",
    "startadmin": "CLIENT_ENV=admin NODE_ENV=production node dist",
    "devserver": "nodemon server --exec babel-node --env-name server",
    "devclient": "parcel watch client/index.html -d dist/public",
    "devadmin": "parcel watch admin/index.html -d dist/public",
    "startadmindev": "CLIENT_ENV=admin concurrently -k \"yarn run devserver\" \"yarn run devadmin\"",
    "start": "concurrently -k \"yarn run devserver\" \"yarn run devclient\""
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "apollo-boost": "^0.1.16",
    "apollo-server-express": "^2.1.0",
    "classnames": "^2.2.6",
    "d3-scale": "^2.1.2",
    "dexie": "^2.0.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.0",
    "mongoose": "^5.7.5",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "semiotic": "1.14.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.8.0",
    "concurrently": "^4.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^24.8.0",
    "nodemon": "^1.18.4",
    "parcel-bundler": "^1.12.3"
  }
}
